{
    "compilerOptions": {
        "noImplicitAny": false,
        "noEmitOnError": true,
        "removeComments": false,
        "sourceMap": true,
        "inlineSources": true,
        // "out": "output/jsstore.js",
        "target": "es5",
        "preserveConstEnums": true,
        // "declaration": true,
        "module": "es6",
        "outDir": "tsc_build"
    },
    "exclude": [
        "node_namespaces",
        "tsc_build"
    ],
    "mapRoot": "./"
}