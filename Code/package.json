{
  "name": "jsstore",
  "version": "1.3.1",
  "description": "JsStore is Sql Like IndexedDb Wrapper. It provides simple api to store, retrieve, delete, remove, and for other advanced Database functionalities.",
  "main": "index.js",
  "scripts": {
    "pretest": "tsc",
    "test": "karma start test/karma.config.js",
    "build": "tsc && uglifyjs output/jsstore.js --compress sequences=true,dead_code=true,conditionals=true,booleans=true,unused=true,if_return=true,join_vars=true,drop_console=true --source-map --mangle --output output/jsstore.min.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ujjwalguptaofficial/JsStore.git"
  },
  "keywords": [
    "jsstore",
    "indexeddb",
    "sql",
    "library",
    "wrapper"
  ],
  "author": "Ujjwal Gupta",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ujjwalguptaofficial/JsStore/issues"
  },
  "homepage": "https://github.com/ujjwalguptaofficial/JsStore#readme",
  "devDependencies": {
    "chai": "^4.1.2",
    "karma": "^1.7.1",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.5",
    "mocha": "^4.0.1",
    "puppeteer": "^0.13.0"
  }
}
